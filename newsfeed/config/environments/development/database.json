{
  "defaultConnection": "default",
  "connections": {
    "default": {
      "connector": "mongoose",
      "settings": {
        "uri": "${process.env.DATABASE_URI || 'mongodb://127.0.0.1:27017' }",
        "database": "${process.env.DATABASE_NAME || 'newsfeed'}"
      },
      "options": {
        "ssl": false
      }
    }
  }
}